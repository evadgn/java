import java.util.Scanner;
public class sumOfOddNumbers {
    public static void main(String[] args) {
        int n;
        int sum = 0;
        Scanner input = new Scanner(System.in);

        do {
            System.out.println("Enter a number : ");
            n = input.nextInt();

            if ( n % 2 == 1){
                sum += n;
            }
        }while ( n > 0 );

        System.out.println("Total : " + sum );
    }
}
