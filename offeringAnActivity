import java.util.Scanner;
public class offeringAnActivity {
    public static void main(String[] args) {
        int heat;
        Scanner input = new Scanner(System.in);

        System.out.println("Enter the temperature of the weather : " );
        heat = input.nextInt();

        if (heat < 5){
            System.out.println("You can go skiing");
        }else if ( 5 <= heat && heat <= 10){
            System.out.println("You can go cinema");
        }else if( 10 < heat && heat <= 15){
            System.out.println("You can go cinema or picnic");
        }else if( 15 < heat && heat <= 25 ){
            System.out.println("You can go picnic");
        }else if( 25 < heat){
            System.out.println("You can go swimming");
        }
    }
}

